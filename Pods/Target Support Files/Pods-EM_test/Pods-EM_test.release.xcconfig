GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/Hyphenate_CN"
LIBRARY_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Hyphenate_CN/HyphenateFullSDK/lib"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/Hyphenate_CN"
OTHER_LDFLAGS = $(inherited) -ObjC -l"HyphenateFullSDK" -l"c++" -l"iconv" -l"sqlite3" -l"stdc++.6.0.9" -l"z" -framework "AVFoundation" -framework "AudioToolbox" -framework "CoreMedia" -framework "ImageIO" -framework "MobileCoreServices"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
